<?xml version="1.0" encoding="UTF-8"?>
<!--
- Months must be provided in chronological order.
- Always provide at least one past month.
- Month ID is of the form: year (4 digits) + dash + month number (1 or 2 digits, no leading zero). 
- If img attribute is not specified, value 'placeholder' is used by default.
- Note that to keep code simple, mobile users are not allowed to navigate to the first month; only to the second.
- Headings (<heading> tag) may be used inside month tags, in between events.
-->
<calendar>
	<month id="2015-4" img="grading1">
		<event date="1, 8, 15">Beginner classes (6:30-7:30)</event>
		<event date="10">Colour-belt grading</event>
		<event date="11">Black-belt grading</event>
	</month>
	<month id="2015-5" img="imag10">
		<event date="3">Movie night</event>
	</month>
	<month id="2015-6" img="camp2">
		<event date="TBC">Training camp</event>
	</month>
	<month id="2015-7" img="fun2">
	</month>
	<month id="2015-8" img="champs5">
		<event date="30">RMIT Champs</event>
	</month>
	<month id="2015-9">
	</month>
</calendar>